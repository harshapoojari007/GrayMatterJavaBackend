<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<display-name>LoginServlet</display-name>
  	<description></description>
  	<servlet-class>com.grayMatter.LoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RegisterServlet</servlet-name>
  	<display-name>RegisterServlet</display-name>
  	<description></description>
  	<servlet-class>com.grayMatter.RegisterServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>OrderSubmission</servlet-name>
  	<display-name>OrderSubmission</display-name>
  	<description></description>
  	<servlet-class>com.grayMatter.OrderSubmission</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ItemServlet</servlet-name>
  	<display-name>ItemServlet</display-name>
  	<description></description>
  	<servlet-class>com.grayMatter.ItemServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RegisterServlet</servlet-name>
  	<url-pattern>/RegisterServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>OrderSubmission</servlet-name>
  	<url-pattern>/OrderSubmission</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ItemServlet</servlet-name>
  	<url-pattern>/ItemServlet</url-pattern>
  </servlet-mapping>
</web-app>
