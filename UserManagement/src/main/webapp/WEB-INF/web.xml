<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>UserInfoServlet</servlet-name>
  	<display-name>UserInfoServlet</display-name>
  	<description></description>
  	<servlet-class>com.grayMatter.UserInfoServlet</servlet-class>
  </servlet>

   <servlet>
  	<servlet-name>WelcomeServlet</servlet-name>
  	<display-name>WelcomeServlet</display-name>
  	<description></description>
  	<servlet-class>com.grayMatter.WelcomeServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ErrorServlet</servlet-name>
  	<display-name>ErrorServlet</display-name>
  	<description></description>
  	<servlet-class>com.grayMatter.ErrorServlet</servlet-class>
  </servlet>

  <servlet>
  	<servlet-name>LoginServ</servlet-name>
  	<display-name>LoginServ</display-name>
  	<description></description>
  	<servlet-class>com.grayMatter.LoginServ</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>UserInfoServlet</servlet-name>
  	<url-pattern>/UserInfoServlet</url-pattern>
  </servlet-mapping>
 
  <servlet-mapping>
  	<servlet-name>ErrorServlet</servlet-name>
  	<url-pattern>/ErrorServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>WelcomeServlet</servlet-name>
  	<url-pattern>/WelcomeServlet</url-pattern>
  </servlet-mapping>
 
  <servlet-mapping>
  	<servlet-name>LoginServ</servlet-name>
  	<url-pattern>/LoginServ</url-pattern>
  </servlet-mapping>
</web-app>
